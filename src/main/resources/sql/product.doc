DROP TABLE product CASCADE;
CREATE TABLE product
(
   product_id(itemcd)              char(20)         NOT NULL,
   description (itemdesc1)         char(100),
   color                           char(30),
   color_category                  char(30),
   series_name                     char(40),
   lable_code                      char(40)  
   inventory_prod_id(invetoryitemcd)  char(20)      DEFAULT,
   abc_code                        char(4),
   category                        char(16),
   origin                          char(18),
   
   --- prod_dimension ---
   length                          char(12),
   width                           char(12),
   thickness                       char(12),
   nominal_length                  char(12),
   nominal_width                   char(12),
   nominal_thickness               char(12),
   size_unit                       char(3),
   thickness_unit                  char(3) ,
   
   product_manager (purchaser1)    char(25),
   buyer (purchaser2)              char(25),    
   mps_code (inactivecd)           char(25),   {01-New Product	 02-Active Product	 03-Pre Drop	 04-Drop	 05-Obsolete	 06-Do Not Inventory	 07-Seconde/Promo	
                                                08- Claims Inventory 09-Direct Ship 10- Special Order}
   grade {First	 Second	 Third}    																																																																										
   status{Best	 Better	 Good}
   
---- prod_material -----
   material_class                  char(5) ,
   material_type                   char(24),
   material_category               char(10),
   material_style                  char(7) ,
   material_feature_1              char(15),
 ? material_feature_2              char(15),
 ? material_feature_3              char(15),

   mbrp(sellprice)                 numeric(9,4),

   prior_mbrp(priorsellprice)      numeric(9,4),
   future_mbrp(futuresellprice)    numeric(9,4),
   ?temp_date_from                 date,
   ?temp_date_thru                 date,
   
   vendor_fob_cost(vendor_fob)     char(10),
   
   ---- prod_package ---
   ---- prod_unit ----
   base_unit                       char(4),
   base_is_std_sell                char(1),
   base_is_std_order               char(1),
   base_is_fract_qty               char(1),
   base_is_pack_unit               char(1),
   base_upc                        numeric(17),
   base_unit_vol                   numeric(10,6),
   base_unit_wgt                   numeric(10,6),
 
