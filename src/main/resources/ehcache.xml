           maxElementsInMemory="1000"
           eternal="true"
           overflowToDisk="true"
           diskPersistent="false"
           diskExpiryThreadIntervalSeconds="2000"
           memoryStoreEvictionPolicy="LRU"/>
   
    <defaultCache
            maxElementsInMemory="20000"
            eternal="false"
            timeToIdleSeconds="1800"
            timeToLiveSeconds="3600"
            overflowToDisk="true"
            diskPersistent="false"
            diskExpiryThreadIntervalSeconds="2000"
            memoryStoreEvictionPolicy="LRU"/>
